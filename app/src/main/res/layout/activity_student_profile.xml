<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Student.StudentProfile">


    <RelativeLayout
        android:id="@+id/relative1"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:background="@color/blue">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="35dp"
            android:background="@mipmap/profile"
            android:visibility="visible" />


        <TextView
            android:id="@+id/txtabc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:text="Nitish Singh"
            android:textColor="@color/white"
            android:textSize="30dp" />

        <TextView
            android:id="@+id/textregno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtabc"
            android:layout_centerHorizontal="true"
            android:text="220970031"
            android:textColor="@color/white"
            android:textSize="20dp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relative2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relative1"
        android:padding="20dp">

        <TextView
            android:id="@+id/enrollNumber"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:text="Enrollment Number" />

        <TextView
            android:id="@+id/enrollNumber1"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_marginLeft="60dp"
            android:layout_toRightOf="@id/enrollNumber"
            android:text=":                     220970031" />


        <TextView
            android:id="@+id/applicationNumber"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/enrollNumber"
            android:text="Application Number" />

        <TextView
            android:id="@+id/applicationNumber1"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/enrollNumber1"
            android:layout_marginLeft="60dp"
            android:layout_toRightOf="@id/applicationNumber"
            android:text=":                     122289191" />


        <TextView
            android:id="@+id/section"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/applicationNumber"
            android:text="Section" />

        <TextView
            android:id="@+id/section1"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/applicationNumber1"
            android:layout_marginLeft="60dp"
            android:layout_toRightOf="@id/section"
            android:text=":                     B" />

        <TextView
            android:id="@+id/academicYear"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/section"
            android:text="Academic Year" />

        <TextView
            android:id="@+id/academicYear1"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/section1"
            android:layout_marginLeft="60dp"
            android:layout_toRightOf="@id/academicYear"
            android:text=":                     2022-2024" />


        <TextView
            android:id="@+id/semester"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/academicYear"
            android:text="Semester" />

        <TextView
            android:id="@+id/semester1"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/academicYear1"
            android:layout_marginLeft="60dp"
            android:layout_toRightOf="@id/semester"
            android:text=":                     |||" />


        <TextView
            android:id="@+id/branch"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/semester"
            android:text="Program/Branch" />

        <TextView
            android:id="@+id/branch1"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/semester1"
            android:layout_marginLeft="60dp"
            android:layout_toRightOf="@id/branch"
            android:text=":                     MCA" />

        <TextView
            android:id="@+id/dob"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/branch"
            android:text="DOB" />

        <TextView
            android:id="@+id/dob1"
            android:layout_width="200dp"
            android:layout_height="42dp"
            android:layout_below="@id/branch1"
            android:layout_marginLeft="60dp"
            android:layout_toRightOf="@id/dob"
            android:text=":                     29/10/2001" />

        <TextView
            android:id="@+id/gender"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/dob"
            android:text="Gender" />

        <TextView
            android:id="@+id/gender1"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/dob1"
            android:layout_marginLeft="60dp"
            android:layout_toRightOf="@id/gender"
            android:text=":                     Male" />


        <TextView
            android:id="@+id/phone"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/gender"
            android:text="Phone Number" />

        <TextView
            android:id="@+id/phone1"
            android:layout_width="200dp"
            android:layout_height="42dp"
            android:layout_below="@id/gender1"
            android:layout_marginLeft="60dp"
            android:layout_toRightOf="@id/phone"
            android:text=":                     9188181911" />


        <TextView
            android:id="@+id/email"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/phone"
            android:text="Email Id" />

        <TextView
            android:id="@+id/email1"
            android:layout_width="200dp"
            android:layout_height="42dp"
            android:layout_below="@id/phone1"
            android:layout_marginLeft="60dp"
            android:layout_toRightOf="@id/email"
            android:text=":                   abc@gmail.com" />


        <TextView
            android:id="@+id/bloodgroup"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/email"
            android:text="Blood Group" />

        <TextView
            android:id="@+id/bloodgroup1"
            android:layout_width="150dp"
            android:layout_height="42dp"
            android:layout_below="@id/email1"
            android:layout_marginLeft="60dp"
            android:layout_toRightOf="@id/bloodgroup"
            android:text=":                     B+" />


    </RelativeLayout>


</RelativeLayout>
